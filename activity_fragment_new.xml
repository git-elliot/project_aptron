<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:orientation="horizontal"
    android:id="@+id/newfraglayout"
    android:layout_weight="9">
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/colorPrimaryDark"
        android:layout_weight="1.5"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_margin="1dp"
            android:background="@drawable/buttonb"
            android:layout_height="match_parent">
            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:onClick="fun"
                android:gravity="bottom|center_horizontal"
                android:typeface="sans"
                android:textStyle="bold"
                android:textColor="@color/Dark_grey_text"
                android:id="@+id/home"
                android:layout_margin="5dp"
                android:background="@drawable/homebl"
                android:layout_weight="9"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:text="Home"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:gravity="bottom|center_horizontal"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_margin="1dp"
            android:background="@drawable/buttonb"
            android:layout_height="match_parent">
            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:onClick="fun"
                android:gravity="bottom|center_horizontal"
                android:typeface="sans"
                android:textStyle="bold"
                android:textColor="@color/Dark_grey_text"
                android:id="@+id/settings"
                android:layout_margin="5dp"
                android:background="@drawable/settings_revamped"
                android:layout_weight="9"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:text="Settings"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:gravity="bottom|center_horizontal"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_margin="1dp"
            android:background="@drawable/buttonb"
            android:layout_height="match_parent">
            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:onClick="fun"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:id="@+id/nearby_help"
                android:layout_margin="5dp"
                android:background="@drawable/nearby_icon"
                android:layout_weight="9"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:text="Nearby Help"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:gravity="bottom|center_horizontal"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_margin="1dp"
            android:background="@drawable/buttonb"
            android:layout_height="match_parent">
            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:onClick="fun"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:id="@+id/guar_details"
                android:layout_margin="5dp"
                android:background="@drawable/acc"
                android:layout_weight="9"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:text="Guardian"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:gravity="bottom|center_horizontal"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:layout_margin="1dp"
            android:background="@drawable/buttonb"
            android:layout_height="match_parent">
            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:textStyle="bold"
                android:onClick="fun"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:id="@+id/feedback"
                android:layout_margin="5dp"
                android:background="@drawable/feedback_icon"
                android:layout_weight="9"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:text="Feedback"
                android:typeface="sans"
                android:textColor="@color/Dark_grey_text"
                android:textStyle="bold"
                android:gravity="bottom|center_horizontal"
                />
        </LinearLayout>

</LinearLayout>
    </LinearLayout>